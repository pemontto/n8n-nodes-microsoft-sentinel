{
  "name": "n8n-nodes-microsoft-sentinel",
  "version": "0.2.0",
  "description": "n8n node for interacting with Microsoft Sentinel",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "pemontto",
    "email": "pemontto@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pemontto/n8n-nodes-microsoft-sentinel.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [
      "dist/credentials/MicrosoftSentinelOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Microsoft/Sentinel/MicrosoftSentinel.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.39.1",
    "prettier": "3.6.2",
    "release-it": "^19.0.6",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "uuid": "^13.0.0"
  }
}
